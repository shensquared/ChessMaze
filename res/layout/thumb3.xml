                android:text="9"
                android:layout_margin="5dp" 
                android:padding="5dp"/>
        </LinearLayout>
    </TableRow>

    <TableRow
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center" >
               <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" >

            <Button
                android:id="@+id/ThumbnailButton34"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="10"
                android:layout_margin="5dp" 
                android:padding="5dp"/>

            <Button
                android:id="@+id/ThumbnailButton35"
